{
  "name": "http",
  "description": "Network requests",
  "icon": "network",
  "color": "purple",
  "daemonSupport": "full",
  "methods": [
    {
      "name": "get",
      "signature": "get(url, options?)",
      "description": "Send a GET request",
      "parameters": [
        {
          "name": "url",
          "type": "string",
          "description": "Target URL"
        },
        {
          "name": "options",
          "type": "object",
          "optional": true,
          "description": "Request options { headers, timeout }"
        }
      ],
      "returns": "Promise<{ status: number, data: string, headers: object }>",
      "returnsDescription": "Object containing status code, response data, and response headers"
    },
