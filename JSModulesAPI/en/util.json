{
  "name": "util",
  "description": "Utility functions",
  "icon": "wrench.and.screwdriver",
  "color": "gray",
  "daemonSupport": "full",
  "methods": [
    {
      "name": "uuid",
      "signature": "uuid()",
      "description": "Generate a unique UUID",
      "returns": "string",
      "returnsDescription": "The generated UUID string"
    },
    {
      "name": "md5",
      "signature": "md5(string)",
      "description": "Calculate the MD5 hash of a string",
      "parameters": [
        {
          "name": "string",
          "type": "string",
          "description": "The input string to hash"
        }
      ],
      "returns": "string",
      "returnsDescription": "The resulting MD5 hash value"
    },
    {
      "name": "base64Encode",
      "signature": "base64Encode(string)",
      "description": "Encode a string to Base64 format",
      "parameters": [
        {
          "name": "string",
          "type": "string",
          "description": "The string to encode"
        }
      ],
      "returns": "string",
      "returnsDescription": "The Base64 encoded string"
    },
    {
      "name": "base64Decode",
      "signature": "base64Decode(string)",
      "description": "Decode a Base64 encoded string",
      "parameters": [
        {
          "name": "string",
          "type": "string",
          "description": "The Base64 string to decode"
        }
      ],
      "returns": "string",
      "returnsDescription": "The decoded original string"
    },
    {
      "name": "formatDate",
      "signature": "formatDate(date, format)",
      "description": "Format a Date object into a string",
      "parameters": [
        {
          "name": "date",
          "type": "Date",
          "description": "The Date object to format"
        },
        {
          "name": "format",
          "type": "string",
          "description": "The format pattern (e.g., 'yyyy-MM-dd')"
        }
      ],
      "returns": "string",
      "returnsDescription": "The formatted date string"
    }
  ]
}