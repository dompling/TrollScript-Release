        {
          "name": "...args",
          "type": "any[]",
          "description": "要输出的内容"
        }
      ],
      "returns": "void"
    },
    {
      "name": "table",
      "signature": "table(data)",
      "description": "以表格形式输出",
      "parameters": [
        {
          "name": "data",
          "type": "object | array",
          "description": "要显示的表格数据"
        }
      ],
      "returns": "void"
    },
    {
      "name": "clear",
      "signature": "clear()",
      "description": "清空控制台",
      "returns": "void"
    }
