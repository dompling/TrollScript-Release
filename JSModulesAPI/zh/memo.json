{
  "name": "memo",
  "description": "备忘录操作（iCloud 同步）",
  "icon": "note.text",
  "color": "yellow",
  "daemonSupport": "full",
  "methods": [
    {
      "name": "create",
      "signature": "create(title, content, tags?)",
      "description": "创建备忘录",
      "parameters": [
        {
          "name": "title",
          "type": "string",
          "description": "备忘录标题"
        },
        {
          "name": "content",
          "type": "string",
          "description": "备忘录内容"
        },
        {
          "name": "tags",
          "type": "[string]",
          "optional": true,
          "description": "标签列表"
        }
      ],
      "returns": "object",
      "returnsDescription": "{ success: 是否成功, id: 备忘录ID, memo: 备忘录对象 }"
    },
    {
      "name": "getAll",
      "signature": "getAll()",
      "description": "获取所有备忘录",
      "returns": "[object]",
      "returnsDescription": "备忘录数组 [{ id: ID, title: 标题, content: 内容, createdAt: 创建时间, updatedAt: 更新时间, tags: 标签 }]"
    },
    {
      "name": "getById",
      "signature": "getById(id)",
      "description": "根据 ID 获取备忘录",
      "parameters": [
        {
          "name": "id",
          "type": "string",
          "description": "备忘录 ID"
        }
      ],
      "returns": "object | null",
      "returnsDescription": "备忘录对象或 null（不存在时）"
    },
    {
      "name": "search",
      "signature": "search(keyword)",
      "description": "搜索备忘录（标题和内容）",
      "parameters": [
        {
          "name": "keyword",
          "type": "string",
          "description": "搜索关键词"
        }
      ],
      "returns": "[object]",
      "returnsDescription": "匹配的备忘录数组"
    },
    {
      "name": "update",
      "signature": "update(id, data)",
      "description": "更新备忘录",
      "parameters": [
        {
          "name": "id",
          "type": "string",
          "description": "备忘录 ID"
        },
        {
          "name": "data",
          "type": "object",
          "description": "更新数据 { title?: 新标题, content?: 新内容, tags?: 新标签 }"
        }
      ],
      "returns": "object",
      "returnsDescription": "{ success: 是否成功, id: 备忘录ID }"
    },
    {
      "name": "delete",
      "signature": "delete(id)",
      "description": "删除备忘录",
      "parameters": [
        {
          "name": "id",
          "type": "string",
          "description": "备忘录 ID"
        }
      ],
      "returns": "object",
      "returnsDescription": "{ success: 是否成功, id: 已删除的ID }"
    },
    {
      "name": "clear",
      "signature": "clear()",
      "description": "清空所有备忘录",
      "returns": "object",
      "returnsDescription": "{ success: 是否成功 }"
    },
    {
      "name": "count",
      "signature": "count()",
      "description": "获取备忘录数量",
      "returns": "number",
      "returnsDescription": "备忘录总数"
    }
  ]
}
